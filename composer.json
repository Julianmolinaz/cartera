{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "laravelcollective/annotations": "^5.2.0",
        "laravelcollective/html": "^5.2.0",
        "nesbot/carbon": "^1.22",
        "laracasts/flash": "^2.0",
        "maatwebsite/excel": "~2.1.0",
        "owen-it/laravel-auditing": "^4.1",
        "yajra/laravel-datatables-oracle": "^6.0"

    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "barryvdh/laravel-dompdf": "0.6.1"
    },
    "autoload": {
        "classmap": [
            "database",
            "app/Helpers",
            "app/Console/Commands"
                    ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helper.php",
            "app/Helpers/sanciones.php",
            "app/Helpers/info_general_helper.php",
            "app/Helpers/info_general_carteras_helper.php",
            "app/Helpers/info_venta_creditos_helper.php",
            "app/Helpers/info_general_funcionarios_helper.php",
            "app/Helpers/procredito.php",
            "app/Helpers/datacredito.php",
            "app/Helpers/info_castigada.php",
            "app/Helpers/info_venta_creditos_por_asesor.php",
            "app/Helpers/info_financiero.php",
            "app/Helpers/no_incluidos.php",
            "app/Helpers/funciones_datacredito.php",
            "app/Helpers/middleware.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
